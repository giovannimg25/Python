edad = int(input("Ingresa la edad que tienes: "))
def EvaluaEdad(edad):
    if edad <0:
        raise TypeError("No existen edades negativas")
    if edad <20:
        return "Eres muy joven"
    elif edad <40:
        return "Eres joven"
    elif edad <65:
        return "Eres mayor"
    elif edad <100:
        return "Cuidate.."
    
print(EvaluaEdad(edad))
